{
  "name": "my-reason-expo-app",
  "reason": {
    "react-jsx": 3
  },
  "bsc-flags": ["-bs-super-errors"],
  "bs-dependencies": [
    "reason-react",
    "reason-react-native",
    "reason-react-navigation",
    "reason-expo"
    // "bsdoc"
    ],
  "sources": [
    {
      "dir": "src",
      "subdirs":true
    }
  ],
  "suffix": ".bs.js",
  "refmt": 3,
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative",
    "shims": {
      "Js": "Js",
      "React": "ReactShim",
      "ReactEvent": "ReactEvent",
      "ReasonPervasives": "ReasonPervasives",
      "ReasonReact": "ReactShim"
    },
    "debug": {
      "all": false
    },
    "exportInterfaces": false
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  }
}
